<script type="text/javascript">
    function memeTimerBanner(element, options) {
        this.defaults = {
            duration: 1000,
            start: (new Date()).getTime(),
            end: (new Date()).getTime(),
            fixed: 3,
            setType: 'text',
            hiddenClass: 'hidden',
            endClass: 'end',
            prefix: '',
            msg: '판매가 종료되었습니다!'
        };

        this.init = function () {
            this.config = $.extend({}, this.defaults, this.config);

            if (this.config.end <= this.config.start) {
                this._end();
            } else {
                // memeWeb-2253
//                    $('.mainVisual').hide();
                this._create();
            }
        };

        this._create = function () {
            var self = this;

            var n = self.config.end - self.config.start;

            if (n <= 0) self._end();

            $(this.element + ' .rightWrap').html(this.formatter(n));

            var intervalId = window.setInterval(function () {
                var start = (new Date()).getTime();
                var end = moment('').valueOf();
                n = end - start;

                if (n === 0 || end <= start) {
                    self._end(intervalId);
                    $(this.element).hide();
//                        $('.mainVisual').css('margin-bottom', '25px');
                }

                $(this.element + ' .rightWrap').html(this.formatter(n));
            }, self.config.duration);


            $(self.element).bind('countdown.stop', function (e) {
                self._stop(intervalId);
            });

            $(self.element).bind('countdown.end', function (e) {
                self._end(intervalId);
                $(self.element + ' .rightWrap').html(this.formatter(n));
            });
        };

        this.formatter = function (time) {
            if (time <= 0) {
                document.href.reload();
            } else {
                var t = this.timer(time);
                var hours = this.zeroFill(t.hours).toString();
                var minutes = this.zeroFill(t.minutes).toString();
                var seconds = this.zeroFill(t.seconds).toString();
                var prefix = $('#timerPrefix').hasClass('imminent');

                if (!prefix && ( (t.hours == 1 && t.minutes == 0 && t.seconds == 0) || t.hours == 0)) {
                    $('#timerPrefix').addClass('imminent');
                }

                return '<span class="time num' + hours.substr(0, 1) + ' fL"></span>' +
                        '<span class="time num' + hours.substr(1, 1) + ' fL"></span>' +
                        '<span class="colon fL"></span>' +
                        '<span class="time num' + minutes.substr(0, 1) + ' fL"></span>' +
                        '<span class="time num' + minutes.substr(1, 1) + ' fL"></span>' +
                        '<span class="colon fL"></span>' +
                        '<span class="sec s_num' + seconds.substr(0, 1) + ' fL"></span>' +
                        '<span class="sec s_num' + seconds.substr(1, 1) + ' fL"></span>';
            }
        };

        this.timer = function (time) {

            var days = time / 1000 / 60 / 60 / 24;
            var daysRound = Math.floor(days);
            var hours = time / 1000 / 60 / 60 - (24 * daysRound);
            var hoursRound = Math.floor(hours);
            var minutes = time / 1000 / 60 - (24 * 60 * daysRound) - (60 * hoursRound);
            var minutesRound = Math.floor(minutes);
            var seconds = time / 1000 - (24 * 60 * 60 * daysRound) - (60 * 60 * hoursRound) - (60 * minutesRound);

            var tmpSecondsRound = seconds.toFixed(this.config.fixed).split('.');
            var secondsRound = tmpSecondsRound[0];
            var millisecondsRound = tmpSecondsRound[1];

            return {
                days: daysRound,
                hours: hoursRound,
                minutes: minutesRound,
                seconds: secondsRound,
                milseconds: millisecondsRound
            }
        };

        this.zeroFill = function (number) {
            return parseInt(number) <= 9 ? '0' + number : number;
        };

        this._stop = function (intervalId) {
            window.clearInterval(intervalId);
        };

        this._end = function (intervalId) {
            this._stop(intervalId);
        };

        this.element = element;
        this._name = 'memeTimerBanner';
        this.config = options;
        this.init();
    }
    memeTimerBanner('.timerBanner', {
        end: moment('').valueOf(),
        msg: '판매가 종료되었습니다.'
    });
</script>


