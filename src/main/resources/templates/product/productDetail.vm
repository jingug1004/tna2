        $('#sum' + itemId).text($.number(toSum));
        $('#sum2' + itemId).text($.number(toSum));

        $('#cartItem1').html("<span id=\"total_txt\">총 상품금액 : </span><span id=\"SumProductP\"> " + $.number(totalSum / 2) + "</span><span id=\"KR_won\">원</span>");
