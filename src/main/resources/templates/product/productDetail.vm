    <div class="dealDescription">

        <div class="descImage">
            <div class="descImageBig">
                <img src="http://sumcrimage.cafe24.com/$!{choiceDetailProduct.product.thumbUrl}"/>
            </div>
        </div><!-- //descImage -->
        <div class="descInformation" data-product-maxBuyLimit="0">
            <div class="infoTop">
                <div class="tag">
                    #if(${choiceDetailProduct.product.productDelivery.freeDelivery} != 0)
                        <span class="freeIcon">
                        배송비 $number.format(${choiceDetailProduct.product.productDelivery.deliveryPrice})원
                    </span> &nbsp;&nbsp;
                    #end
                    #if(${choiceDetailProduct.product.productDelivery.freeDelivery} == 0)
                        <span class="freeIcon">무료배송</span>
                    #elseif(${choiceDetailProduct.product.productDelivery.freeDelivery} >= 1)
                        <span class="freeIcon">
                        $number.format(${choiceDetailProduct.product.productDelivery.freeDelivery})원 이상 무료배송
                    </span>
                    #end
                </div>
            </div>

            <h1 class="infoTitle">
                <small class="titSub">${choiceDetailProduct.product.prodDesc}</small>
                [${choiceDetailProduct.product.brand.brandName}] ${choiceDetailProduct.product.prodName}
            </h1>

            <div class="infoPrice">
                <dl>
                    <dt class="hidden">가격</dt>
                    <dd class="priceCommon">
                        <strong class="numTxt nuNumber">$number.format(${choiceDetailProduct.product.price})</strong>
                        <span class="nuUnit">원</span>
                    </dd>

                    <dd class="priceSell">
                        <strong class="numTxt nuNumber">$number.format(${choiceDetailProduct.product.salePrice})</strong>
                        <span class="nuUnit">원<!--&sim;--></span>
                    </dd>

                    <dt class="hidden">할인율</dt>
                    <dd class="discountRate">
                        <strong class="nuNumber rateNum">
                            #if(${choiceDetailProduct.product.sale} != 0)
                                ~${choiceDetailProduct.product.sale} <span class="percentage_symbol">%</span>

                            #end
                        </strong>
                    </dd>
                </dl>
            </div>

            <div class="infoAdditional">
                <dl class="infoAdditionalInner">
                    <dt class="additionalTit">배송비</dt>
                    <dd class="additionalDesc">
                        #if(${choiceDetailProduct.product.productDelivery.freeDelivery} != 0)
                            <span class="freeIcon">
                            배송비 $number.format(${choiceDetailProduct.product.productDelivery.deliveryPrice})
                            원
                        </span> &nbsp;&nbsp;
                        #end

                        #if(${choiceDetailProduct.product.productDelivery.freeDelivery} == 0)
                            <span class="freeIcon">무료배송</span>
                        #elseif(${choiceDetailProduct.product.productDelivery.freeDelivery} >= 1)
                            <span class="freeIcon">
                            $number.format(${choiceDetailProduct.product.productDelivery.freeDelivery})원 이상 무료배송
                        </span>
                        #end
                    </dd>

                    <dt class="additionalTit">고객리뷰</dt>
                    <dd class="additionalDesc descReview">
                        <span class="star">
                            <em class="five"></em>
                        </span>
                        <strong id="rPoint"
                                class="numTxt nuNumber">${choiceDetailProduct.product.reviewStar}</strong>
                        <span class="nuUnit">점,</span>
                        <a href="#review" class="descToReview">
                            <strong id="rCnt"
                                    class="numTxt nuNumber">${choiceDetailProduct.product.reviewCount}</strong><span
                                class="nuUnit">건</span>
                        </a>
                    </dd>
                </dl>
            </div>

            <div class="descInfoOption">
