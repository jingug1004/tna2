<script type="text/javascript">
		var awardList = $('#awardList').bxSlider({
			slideWidth : 178, // 310
			minSlides : 4,
			maxSlides : 4,
			moveSlides : 1,
			slideMargin : 0,
			nextSelector : '#btn_award_prev',
			prevSelector : '#btn_award_next',
			nextText : '',
			prevText : '',
			pager : false
		});

		$('#award_arrow_next')
				.click(
						function(event) {
							var currentTarget = $(event.currentTarget);
							if (currentTarget.hasClass('award_next_on')) {
								awardList.goToNextSlide();
							}

							if (awardList.getSlideCount()
									- awardList.getCurrentSlide() == 4) {
								currentTarget.removeClass('award_next_on')
										.addClass('award_next');
							} else if (currentTarget.hasClass('award_next')) {
								currentTarget.removeClass('award_next')
										.addClass('award_next_on');
							}

							if (awardList.getCurrentSlide() != 0) {
								currentTarget.parents().find(
										'#award_arrow_prev').removeClass(
										'award_prev').addClass('award_prev_on');
							}
						});

		$('#award_arrow_prev')
				.click(
						function(event) {
							var currentTarget = $(event.currentTarget);
							if (awardList.getCurrentSlide() != 0
									&& $('#award_arrow_prev').hasClass(
											'award_prev_on')) {
								awardList.goToPrevSlide();
							}

							if (awardList.getCurrentSlide() == 0) {
								currentTarget.removeClass('award_prev_on')
										.addClass('award_prev');
							} else {
								currentTarget.removeClass('award_prev')
										.addClass('award_prev_on');
							}

							if (awardList.getSlideCount()
									- awardList.getCurrentSlide() != 4) {
								currentTarget.parents().find(
										'#award_arrow_next').removeClass(
										'award_next').addClass('award_next_on');
							}
						});
</script>

