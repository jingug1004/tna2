									var promotion_name = $(this).data(
											"promotion-name");
									var promotion_creative = $(this).data(
											"promotion-creative");
									var promotion_link = $(this).data(
											"promotion-link");
									var promotion_position = $(this).data(
											"promotion-position");
									//                analytics.track("Promotion Viewed", {
